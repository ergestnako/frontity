{
  "name": "typescript-basic",
  "version": "1.0.1",
  "private": true,
  "description": "The minimum setup for Frontity using TypeScript",
  "scripts": {
    "dev": "ts-node ./node_modules/frontity/src/commands.ts dev",
    "dev:inspect": "node --inspect -r ts-node/register ./node_modules/frontity/src/commands.ts dev",
    "build": "ts-node ./node_modules/frontity/src/commands.ts build",
    "build:inspect": "node --inspect -r ts-node/register ./node_modules/frontity/src/commands.ts build",
    "serve": "ts-node ./node_modules/frontity/src/commands.ts serve",
    "serve:inspect": "node --inspect -r ts-node/register ./node_modules/frontity/src/commands.ts serve"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@frontity/core": "^0.2.0",
    "@frontity/file-settings": "^0.1.0",
    "@frontity/extension-example-1": "^1.0.0",
    "@frontity/extension-example-2": "^1.0.0",
    "frontity": "^0.1.4"
  },
  "devDependencies": {
    "ts-node": "^8.0.3"
  }
}
